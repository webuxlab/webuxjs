{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error"
  }
}
