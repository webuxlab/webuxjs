{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {},
  "overrides": [
    {
      "files": "*",
      "rules": {
        "new-cap": "off",
        "require-jsdoc": "off",
        "max-len": "off",
        "indent": "off"
      }
    }
  ]
}
